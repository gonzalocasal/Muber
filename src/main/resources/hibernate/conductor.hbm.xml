<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="ar.com.grupo27.model.Conductor" table="CONDUCTOR">
		<id name="id" type="int" column="ID_CONDUCTOR" >
			<generator class="native"/>
		</id>
	 	<property name="usuario" column="USUARIO" type="string"/>
	 	<property name="nombre" column="NOMBRE" type="string"/>
	 	<property name="apellido" column="APELLIDO" type="string"/>
	 	<property name="fechaAlta" column="FECHA_ALTA" type="date"/>
	 	<property name="fechaExipracionLicencia" column="FECHA_EXPIRACION_LICENCIA" type="date"/>
		
		<bag name="viajes" fetch="select">
          <key column="ID_CONDUCTOR" />
          <one-to-many class="ar.com.grupo27.model.Viaje"/>
      	</bag>
      	
	</class>
     
</hibernate-mapping>
